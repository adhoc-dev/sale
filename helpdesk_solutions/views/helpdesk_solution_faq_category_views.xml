<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record model="ir.ui.view" id="helpdesk_solution_faq_category_view">
        <field name="name">Helpdesk Solution FAQ Category Form</field>
        <field name="model">helpdesk.solution.faq.category</field>
        <field name="arch" type="xml">
           <form>
                <group>
                    <field name="name"/>
                </group>
            </form>
        </field>
    </record>

    <record model="ir.actions.act_window" id="helpdesk_solution_faq_category_action">
        <field name="name">Solutions FAQ Categories</field>
        <field name="res_model">helpdesk.solution.faq.category</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" eval="False"/>
        <field name="domain" eval=""/>
    </record>

    <menuitem id="menu_helpdesk_solution_faq_category"
        parent="helpdesk.helpdesk_menu_config"
        action="helpdesk_solution_faq_category_action" sequence="35"/>

</odoo>
