<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="helpdesk_ticket_1" model="helpdesk.ticket">
            <field name="name">White cabinets</field>
            <field name="team_id" ref="helpdesk.helpdesk_team1"/>
            <field name="priority">1</field>
            <field name="stage_id" ref="helpdesk.stage_in_progress"/>
            <field name="user_id" ref="base.user_admin"/>
            <field name="partner_id" ref="base.partner_demo_portal"/>
            <field name="description" type="html">
<p>Hello,<br/><br/>
I would like to know if your cabinets come in white color?<br/><br/>
Thank you,<br/>
Joel Willis</p>
            </field>
        </record>
        <record id="helpdesk_ticket_2" model="helpdesk.ticket">
            <field name="name">The cabinet doors has a defect</field>
            <field name="team_id" ref="helpdesk.helpdesk_team1"/>
            <field name="priority">3</field>
            <field name="stage_id" ref="helpdesk.stage_in_progress"/>
            <field name="user_id" ref="base.user_admin"/>
            <field name="partner_id" ref="base.partner_demo_portal"/>
            <field name="description" type="html">
<p>Hello,<br/><br/>
I have purchased a "Cabinet" from your store but the doors have a defect.
Would it be possible for you to change it?<br/><br/>
Regards,<br/>
Joel Willis</p>
            </field>
        </record>
        <record id="helpdesk_ticket_3" model="helpdesk.ticket">
            <field name="name">What measurements are available for kitchen tables?</field>
            <field name="team_id" ref="helpdesk.helpdesk_team1"/>
            <field name="priority">1</field>
            <field name="stage_id" ref="helpdesk.stage_done"/>
            <field name="user_id" ref="base.user_demo"/>
            <field name="partner_id" ref="base.partner_demo_portal"/>
            <field name="description" type="html">
<p>Hi,<br/><br/>
I would like to know the measurements of the tables.<br/><br/>
Thank you,<br/>
Joel Willis</p>
            </field>
        </record>
    </data>
</odoo>
