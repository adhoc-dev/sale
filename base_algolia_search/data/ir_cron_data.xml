<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="cron_algolia_index_sync" model="ir.cron">
        <field name="name">Algolia Index Sync</field>
        <field name="model_id" ref="base.model_ir_model"/>
        <field name="state">code</field>
        <field name="code">model._cron_algolia_index_sync()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="nextcall" eval="DateTime.now().strftime('%Y-%m-%d 20:%M:%S')"/>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
    </record>

</odoo>
